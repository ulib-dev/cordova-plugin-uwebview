{
    "name": "cordova-plugin-uwebview",
    "version": "2.0.17",
    "description": "Cordova UWebView Plugin",
    "types": "./index.d.ts",
    "cordova": {
        "id": "cordova-plugin-uwebview",
        "platforms": [
            "android",
            "ios"
        ]
    },
    "repository": "https://github.com/ulib-dev/cordova-plugin-uwebview",
    "bugs": "https://github.com/ulib-dev/cordova-plugin-uwebview/issues",
    "keywords": [
        "cordova",
        "uwebview"
    ],
    "scripts": {
        "test": "npm run jshint",
        "jshint": "node node_modules/jshint/bin/jshint www && node node_modules/jshint/bin/jshint src && node node_modules/jshint/bin/jshint tests",
        "start": "webpack serve --open --mode development",
        "build": "webpack --mode production && node copyFile.js",
        "build:n": "webpack --mode production && node HtmlInlineAndCopyFile.js",
        "build:uplayer": "webpack --mode production --config webpack.config4uplayer.js && node copyUplayer.js",
        "build:all": "npm run build && npm run build:uplayer"
    },
    "dependencies": {
        "plyr": "^3.7.8"
    },
    "engines": {
        "cordovaDependencies": {}
    },
    "author": "Apache Software Foundation",
    "license": "Apache-2.0",
    "devDependencies": {
        "@babel/core": "^7.25.8",
        "@babel/preset-env": "^7.25.8",
        "@fortawesome/fontawesome-free": "^6.6.0",
        "babel-loader": "^9.2.1",
        "copy-webpack-plugin": "^12.0.2",
        "cp-cli": "^2.0.0",
        "css-loader": "^7.1.2",
        "file-loader": "^6.2.0",
        "html-inline-css-webpack-plugin": "^1.11.2",
        "html-inline-script-webpack-plugin": "^3.2.1",
        "html-webpack-plugin": "^5.6.0",
        "jshint": "^2.6.0",
        "mini-css-extract-plugin": "^2.9.1",
        "style-loader": "^4.0.0",
        "url-loader": "^4.1.1",
        "video.js": "^8.18.1",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    }
}